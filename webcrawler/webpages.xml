<?xml version="1.0"?>
<webpages>
	
	<webpage url="dn.se">

		<article-qualification>
			<xpath>div[@id="article"]</xpath>
			<xpath>div/p[@class="header"]</xpath>
		</article-qualification>
		
		<article-data>
			<title>
				<xpath>//div[@id="article-content"]/h1/text()</xpath>
			</title>
		
			<summary>
				<xpath>//div[@id="article-content"]/div[@class="article-wrapper"]/div[@class="content-before"]</xpath>
			</summary>
		</article-data>

	</webpage>

</webpages>

